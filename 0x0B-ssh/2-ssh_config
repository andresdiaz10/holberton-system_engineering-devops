# Client configuration file

Include /etc/ssh/ssh_config.d/*.conf

Host *
    Host 35.190.173.167
    HostName 35.190.173.167
    User ubuntu
    PasswordAuthentication no
    IdentityFile ~/.ssh/school
    SendEnv LANG LC_*
    HashKnownHosts yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials no
